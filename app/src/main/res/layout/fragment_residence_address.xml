<?xml version="1.0" encoding="utf-8"?>


<ScrollView xmlns:tools="http://schemas.android.com/tools"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:id="@+id/scrollView2">
    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


        <TextView
                android:id="@+id/tvCurrentResidence"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:textSize="20dp"
                android:textColor="@color/black"
                android:text="@string/currentresidence" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintBottom_toTopOf="@+id/tvHouse"
                app:layout_constraintVertical_chainStyle="spread_inside" android:layout_marginBottom="32dp"/>

        <TextView
                android:id="@+id/tvHouse"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="House/Flat/Wing/Building"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintTop_toBottomOf="@+id/tvCurrentResidence"
                app:layout_constraintBottom_toTopOf="@+id/edHouse" android:layout_marginBottom="16dp"/>

        <ImageView
                android:id="@+id/ivHouse"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/user"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintEnd_toStartOf="@+id/edHouse"
                app:layout_constraintTop_toTopOf="@+id/edHouse" app:layout_constraintBottom_toBottomOf="@+id/edHouse"/>

        <EditText
                android:id="@+id/edHouse"
                android:layout_width="320dp"
                android:layout_height="50dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toEndOf="@+id/ivHouse" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toBottomOf="@+id/tvHouse"
                app:layout_constraintBottom_toTopOf="@+id/tvExResidence" android:layout_marginBottom="8dp"/>

        <TextView
                android:id="@+id/tvExResidence"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="Example:1703, Building 3,Raheja Optima Society"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintTop_toBottomOf="@+id/edHouse" app:layout_constraintBottom_toTopOf="@+id/tvStreet"
                android:layout_marginBottom="24dp"/>

        <TextView
                android:id="@+id/tvStreet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="Street Address"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintTop_toBottomOf="@+id/tvExResidence"
                app:layout_constraintBottom_toTopOf="@+id/edStreet" android:layout_marginBottom="16dp"/>

        <ImageView
                android:id="@+id/imStreet"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/user"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/edStreet"
                app:layout_constraintTop_toTopOf="@+id/edStreet"
                app:layout_constraintBottom_toBottomOf="@+id/edStreet"/>

        <EditText
                android:id="@+id/edStreet"
                android:layout_width="320dp"
                android:layout_height="50dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toEndOf="@+id/imStreet" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toBottomOf="@+id/tvStreet"
                app:layout_constraintBottom_toTopOf="@+id/tvExStreet" android:layout_marginBottom="8dp"/>

        <TextView
                android:id="@+id/tvExStreet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="Example:Second Cross Lane"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintTop_toBottomOf="@+id/edStreet"
                app:layout_constraintHorizontal_bias="0.0" app:layout_constraintBottom_toTopOf="@+id/tvLocality"
                android:layout_marginBottom="24dp"/>

        <TextView
            android:id="@+id/tvLocality"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            android:paddingLeft="15dp"
            android:text="Locality Name(optional if street address given)"
            app:layout_constraintBottom_toTopOf="@+id/edLocality"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvExStreet" />

        <ImageView
                android:id="@+id/imLocality"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/location"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/edLocality"
                app:layout_constraintTop_toTopOf="@+id/edLocality"
                app:layout_constraintBottom_toBottomOf="@+id/edLocality"
                app:layout_constraintHorizontal_chainStyle="spread"/>

        <EditText
                android:id="@+id/edLocality"
                android:layout_width="320dp"
                android:layout_height="50dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toEndOf="@+id/imLocality" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toBottomOf="@+id/tvLocality"
                app:layout_constraintBottom_toTopOf="@+id/tvExLocality" android:layout_marginBottom="8dp"/>

        <TextView
                android:id="@+id/tvExLocality"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="Example:Lokhandawala, Andheri West"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintTop_toBottomOf="@+id/edLocality"
                app:layout_constraintHorizontal_bias="0.0" app:layout_constraintBottom_toTopOf="@+id/tvNearBy"
                android:layout_marginBottom="24dp"/>

        <TextView
                android:id="@+id/tvNearBy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="Nearby Landmark"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.01"
                app:layout_constraintTop_toBottomOf="@+id/tvExLocality"
                app:layout_constraintBottom_toTopOf="@+id/edNearBy" android:layout_marginBottom="16dp"/>

        <ImageView
                android:id="@+id/imNearBy"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/location"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/edNearBy"
                app:layout_constraintTop_toTopOf="@+id/edNearBy"
                app:layout_constraintBottom_toBottomOf="@+id/edNearBy"/>

        <EditText
                android:id="@+id/edNearBy"
                android:layout_width="320dp"
                android:layout_height="50dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toEndOf="@+id/imNearBy" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toBottomOf="@+id/tvNearBy"
                app:layout_constraintBottom_toTopOf="@+id/tvExNearBy" android:layout_marginBottom="8dp"/>

        <TextView
                android:id="@+id/tvExNearBy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="Example: Opp Infinity Mall"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintTop_toBottomOf="@+id/edNearBy"
                app:layout_constraintHorizontal_bias="0.0" app:layout_constraintBottom_toTopOf="@+id/tvPinCode"
                android:layout_marginBottom="24dp"/>

        <TextView
                android:id="@+id/tvPinCode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="Pin Code"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.04"
                app:layout_constraintTop_toBottomOf="@+id/tvExNearBy"
                app:layout_constraintBottom_toTopOf="@+id/edPinCode" android:layout_marginBottom="16dp"/>

        <ImageView
                android:id="@+id/imPinCode"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/location"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/edPinCode"
                app:layout_constraintTop_toTopOf="@+id/edPinCode"
                app:layout_constraintBottom_toBottomOf="@+id/edPinCode"/>

        <EditText
                android:id="@+id/edPinCode"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:hint="Pin"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toEndOf="@+id/imPinCode" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toBottomOf="@+id/tvPinCode" app:layout_constraintBottom_toTopOf="@+id/tvCity"
                android:layout_marginBottom="16dp"/>

        <TextView
                android:id="@+id/tvCity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="City"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintTop_toBottomOf="@+id/edPinCode" app:layout_constraintBottom_toTopOf="@+id/edCity"
                android:layout_marginBottom="16dp"/>

        <ImageView
                android:id="@+id/imCity"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/location"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/edCity"
                app:layout_constraintTop_toTopOf="@+id/edCity" app:layout_constraintBottom_toBottomOf="@+id/edCity"/>

        <Spinner
                android:id="@+id/edCity"
                android:layout_width="320dp"
                android:layout_height="50dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toEndOf="@+id/imCity" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toBottomOf="@+id/tvCity" app:layout_constraintBottom_toTopOf="@+id/tvState"
                android:layout_marginBottom="24dp"/>

        <TextView
                android:id="@+id/tvState"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:text="State"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintTop_toBottomOf="@+id/edCity" app:layout_constraintBottom_toTopOf="@+id/edState"
                android:layout_marginBottom="16dp"/>

        <ImageView
                android:id="@+id/imState"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/location"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/edState"
                app:layout_constraintHorizontal_chainStyle="spread_inside" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="@+id/edState"/>

        <EditText
                android:id="@+id/edState"
                android:layout_width="320dp"
                android:layout_height="50dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toEndOf="@+id/imState" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintTop_toBottomOf="@+id/tvState" app:layout_constraintBottom_toTopOf="@+id/button"
                android:layout_marginBottom="40dp"/>

        <Button
                android:id="@+id/button"
                android:layout_width="316dp"
                android:layout_height="45dp"
                android:background="@color/orange"
                android:text="Submit"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/edState"
                app:layout_constraintVertical_bias="1.0" android:layout_marginBottom="8dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"/>


    </android.support.constraint.ConstraintLayout>
</ScrollView>